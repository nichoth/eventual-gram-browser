{
  "name": "ssb-browser-demo",
  "description": "",
  "version": "0.0.0",
  "main": "src/index.js",
  "dependencies": {
    "@nichoth/photos": "^0.0.2",
    "bipf": "^1.4.0",
    "minisearch": "^3.0.2",
    "preact": "^10.5.13",
    "pull-level": "^2.0.4",
    "pull-stream": "^3.6.14",
    "ssb-browser-core": "^9.2.0",
    "ssb-db2": "^1.18.6",
    "ssb-threads": "^7.0.0"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "budo": "^11.6.4",
    "concurrently": "^6.0.0",
    "esmify": "^2.1.1",
    "sass": "^1.32.8",
    "tap-spec": "^5.0.0",
    "tape": "^5.2.2",
    "tape-run": "^8.0.0"
  },
  "scripts": {
    "start": "npm run sass && concurrently --kill-others \"npm run serve\" \"npm run sass-watch\"",
    "serve": "budo src/index.js:bundle.js --pushstate --dir=public --live --open -- -p esmify --debug",
    "sass": "sass ./src/style/style.scss ./public/style.css",
    "sass-watch": "sass --watch src/style:public",
    "build": "browserify -p esmify --debug ./src/index.js > public/bundle.js",
    "test": "tape test/*.js | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/ssb-browser-demo.git"
  },
  "author": "Nick Thomas <nichoth@gmail.com>",
